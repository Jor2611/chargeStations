{
  "name": "chargestation",
  "version": "1.0.0",
  "description": "Electric vehicle charging station management system REST ",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f ./config/dev.env node src/index.js",
    "dev": "env-cmd -f ./config/dev.env nodemon src/index.js",
    "test": "env-cmd -f ./config/test.env jest --watch --runInBand",
    "db": "env-cmd -f ./config/dev.env node config/db.js",
    "container": "npm run db && npm start"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "REST"
  ],
  "author": "Zhora Khachatryan",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "geolib": "^3.0.4",
    "lodash": "^4.17.19",
    "mongoose": "^5.7.7",
    "mongoose-double": "0.0.1",
    "uuid": "^3.3.3",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2"
  }
}
